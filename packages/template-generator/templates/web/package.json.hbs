{
  "name": "{{#if (eq structure 'monorepo')}}@repo/web{{else}}{{projectName}}{{/if}}",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "{{#if (eq structure 'monorepo')}}next dev --turbopack{{else}}node scripts/dev.mjs{{/if}}",
    "dev:next": "next dev --turbopack",
{{#unless (eq structure 'monorepo')}}    "dev:setup": "npx convex dev --configure --until-success",
{{/unless}}    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
{{#if (eq structure 'monorepo')}}    "@repo/backend": "workspace:*",
    "@repo/ui": "workspace:*",
{{/if}}    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "convex": "^1.25.0",
    "@convex-dev/better-auth": "^0.10.0",
    "better-auth": "1.4.9",
{{#unless (eq structure 'monorepo')}}{{#if (eq shadcn.iconLibrary "hugeicons")}}    "@hugeicons/react": "^0.3.0",
{{/if}}{{#if (eq shadcn.iconLibrary "lucide")}}    "lucide-react": "^0.469.0",
{{/if}}{{#if (eq shadcn.iconLibrary "tabler")}}    "@tabler/icons-react": "^3.31.0",
{{/if}}{{#if (eq shadcn.iconLibrary "phosphor")}}    "@phosphor-icons/react": "^2.1.7",
{{/if}}{{/unless}}    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.5.0",
    "tw-animate-css": "^1.3.0",
    "resend": "^4.0.0",
    "react-email": "^3.0.0",
    "@react-email/components": "^0.0.36"{{#if (eq integrations.analytics 'posthog')}},
    "posthog-js": "^1.200.0",
    "posthog-node": "^5.0.0"{{/if}}{{#if (eq integrations.analytics 'vercel')}},
    "@vercel/analytics": "^1.4.0",
    "@vercel/speed-insights": "^1.1.0"{{/if}}{{#if (eq integrations.uploads 'uploadthing')}},
    "uploadthing": "^7.0.0",
    "@uploadthing/react": "^7.0.0"{{/if}}{{#if (eq integrations.uploads 's3')}},
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0"{{/if}}{{#if (eq integrations.uploads 'vercel-blob')}},
    "@vercel/blob": "^2.0.0"{{/if}}{{#if (includes addons 'rate-limiting')}},
    "@arcjet/next": "^1.0.0-beta.16"{{/if}}{{#if (includes addons 'monitoring')}},
    "@sentry/nextjs": "^8.0.0"{{/if}}
  },
  "devDependencies": {
{{#if (eq structure 'monorepo')}}    "@repo/config-typescript": "workspace:*",
{{/if}}    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "postcss": "^8.4.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@testing-library/react": "^16.0.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.50.0",
    "@playwright/test": "^1.50.0"
  }
